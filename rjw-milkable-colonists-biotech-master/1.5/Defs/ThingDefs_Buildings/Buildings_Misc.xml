<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="FurnitureBase">
    <defName>MilkingStation</defName>
    <label>milking station</label>
    <description>A table that allows a user to more conveniently milk themselves.</description>
    <thingClass>Building_Storage</thingClass>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Building/Furniture/Stool</texPath>
      <drawSize>(1,1)</drawSize>
      <shadowData>
        <volume>(0.8, 1, 0.8)</volume>
      </shadowData>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <designationCategory>Production</designationCategory>
    <uiOrder>7778</uiOrder>
    <fillPercent>0.35</fillPercent>
    <selectable>true</selectable>
    <useHitPoints>true</useHitPoints>
    <size>(1,1)</size>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>50</costStuffCount>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1000</WorkToBuild>
      <Beauty>10</Beauty>
      <Flammability>1.0</Flammability>
      <Mass>20</Mass>
    </statBases>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <building>
      <buildingTags>
        <li>Production</li>
      </buildingTags>
      <paintable>true</paintable>
	  <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
  
      <fixedStorageSettings>
        <filter>
          <disallowNotEverStorable>true</disallowNotEverStorable>
          <thingDefs>
            <li>HumanoidMilk</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
          <disallowNotEverStorable>true</disallowNotEverStorable>
          <thingDefs>
            <li>HumanoidMilk</li>
          </thingDefs>
        </filter>
      </defaultStorageSettings>
    </building>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <comps>
	  <li MayRequire="DRILLEDHEAD.MILKEXPANDED" Class="PipeSystem.CompProperties_ConvertThingToResource">
		<pipeNet>Milk_pipeNet</pipeNet>
		<thing>HumanoidMilk</thing>
	  </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>True</drawPlaceWorkersWhileSelected>
    <researchPrerequisites>
      <li>Smithing</li>
    </researchPrerequisites>
  </ThingDef>
  


</Defs>
