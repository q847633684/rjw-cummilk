            </outcomeDoers>
        </ingestible>
        <costList>
            <Neutroamine>2</Neutroamine>
            <MedicineHerbal>1</MedicineHerbal>
        </costList>
    </ThingDef>

    <ThingDef ParentName="EM_DrugBase">
