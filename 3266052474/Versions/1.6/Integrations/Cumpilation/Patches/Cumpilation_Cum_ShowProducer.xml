<?xml version="1.0" encoding="utf-8"?>
<!-- When Cumpilation (vegapnk.cumpilation) is active: add CompShowProducer to Cumpilation_Cum so we can tag producer and apply same consumption rules as milk (who can eat/use). -->
<Patch>
  <Operation Class="PatchOperationAdd">
    <xpath>Defs/ThingDef[defName="Cumpilation_Cum"]</xpath>
    <value>
      <comps>
        <li Class="EqualMilking.CompProperties_ShowProducer" />
      </comps>
    </value>
  </Operation>
</Patch>
