<Defs>
    <DesignatorDropdownGroupDef>
        <defName>EM_MilkTaps</defName>
        <label>milk taps</label>
    </DesignatorDropdownGroupDef>
    <ThingDef ParentName="BuildingBase" Name="EM_MilkTapBase" Abstract="True">
        <thingClass>Building_Storage</thingClass>
        <drawerType>MapMeshAndRealTime</drawerType>
        <designatorDropdown>EM_MilkTaps</designatorDropdown>
        <rotatable>false</rotatable>
        <altitudeLayer>Building</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <fillPercent>0.5</fillPercent>
        <castEdgeShadows>true</castEdgeShadows>
        <pathCost>50</pathCost>
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Mass>8</Mass>
            <WorkToBuild>800</WorkToBuild>
            <Flammability>0.5</Flammability>
        </statBases>
        <size>(1,1)</size>
        <costList>
            <Steel>20</Steel>
        </costList>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <isHopper>True</isHopper>
        </building>
        <inspectorTabs>
            <li>ITab_Storage</li>
        </inspectorTabs>
        <staticSunShadowHeight>0.5</staticSunShadowHeight>
        <surfaceType>Item</surfaceType>
        <canOverlapZones>false</canOverlapZones>
        <comps>
            <li MayRequire="VanillaExpanded.VNutrientE"
                Class="VNPE.CompProperties_RegisterToGrinder" />
        </comps>
    </ThingDef>
    <ThingDef ParentName="EM_MilkTapBase">
        <defName>EM_MilkTap</defName>
        <label>milk tap</label>
        <description>A tap for pouring milk from the connected tanks.</description>
        <graphicData>
            <texPath>Things/Building/Storage/MilkTap</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
            </damageData>
            <drawRotated>false</drawRotated>
            <allowFlip>false</allowFlip>
            <attachments>
                <li>
                    <texPath>things/item/resource/milk</texPath>
                    <graphicClass>Graphic_Single</graphicClass>
                    <drawSize>(0.3,0.3)</drawSize>
                    <drawOffset>(0,0.01,-0.32)</drawOffset>
                    <shaderType>EdgeDetect</shaderType>
                    <shaderParameters>
                        <_Color>(0.42,0.33,0.2,1)</_Color>
                    </shaderParameters>
                </li>
            </attachments>
        </graphicData>
        <building>
            <fixedStorageSettings>
                <filter>
                    <disallowNotEverStorable>true</disallowNotEverStorable>
                    <thingDefs>
                        <li>Milk</li>
                    </thingDefs>
                </filter>
            </fixedStorageSettings>
            <defaultStorageSettings>
                <priority>Important</priority>
                <filter>
                    <disallowNotEverStorable>true</disallowNotEverStorable>
                    <thingDefs>
                        <li>Milk</li>
                    </thingDefs>
                </filter>
            </defaultStorageSettings>
        </building>
        <comps>
            <li Class="PipeSystem.CompProperties_ConvertResourceToThing">
                <pipeNet>EM_MilkNet</pipeNet>
                <thing>Milk</thing>
            </li>
            <li Class="PipeSystem.CompProperties_Resource">
                <pipeNet>EM_HumanMilkNet</pipeNet>
            </li>
        </comps>
        <designationCategory>EM_PipeNetworks</designationCategory>
    </ThingDef>
    <ThingDef ParentName="EM_MilkTapBase">
        <defName>EM_HumanMilkTap</defName>
        <label>human milk tap</label>
        <description>A tap for pouring human milk from the connected tanks.</description>
        <graphicData>
            <texPath>Things/Building/Storage/MilkTap</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
            </damageData>
            <drawRotated>false</drawRotated>
            <allowFlip>false</allowFlip>
            <attachments>
                <li>
                    <texPath>Things/Item/Ingestible/HumanMilk/HumanMilk.tex</texPath>
                    <graphicClass>Graphic_Single</graphicClass>
                    <drawSize>(0.3,0.3)</drawSize>
                    <drawOffset>(0,0.01,-0.32)</drawOffset>
                    <shaderType>EdgeDetect</shaderType>
                    <shaderParameters>
                        <_Color>(0.42,0.33,0.2,1)</_Color>
                    </shaderParameters>
                </li>
            </attachments>
        </graphicData>
        <building>
            <fixedStorageSettings>
                <filter>
                    <disallowNotEverStorable>true</disallowNotEverStorable>
                    <thingDefs>
                        <li>EM_HumanMilk</li>
                    </thingDefs>
                </filter>
            </fixedStorageSettings>
            <defaultStorageSettings>
                <priority>Important</priority>
                <filter>
                    <disallowNotEverStorable>true</disallowNotEverStorable>
                    <thingDefs>
                        <li>EM_HumanMilk</li>
                    </thingDefs>
                </filter>
            </defaultStorageSettings>
        </building>
        <comps>
            <li Class="PipeSystem.CompProperties_ConvertResourceToThing">
                <pipeNet>EM_HumanMilkNet</pipeNet>
                <thing>EM_HumanMilk</thing>
            </li>
            <li Class="PipeSystem.CompProperties_Resource">
                <pipeNet>EM_MilkNet</pipeNet>
            </li>
        </comps>
        <designationCategory>EM_PipeNetworks</designationCategory>
    </ThingDef>
</Defs>